{
  "mcpServers": {
    "jira": {
      "command": "jira-mcp-server",
      "env": {
        "ATLASSIAN_HOST": "https://your-company.atlassian.net",
        "ATLASSIAN_EMAIL": "user@company.com",
        "ATLASSIAN_TOKEN": "your-api-token"
      }
    },
    "clickup": {
      "command": "clickup-mcp-server",
      "env": {
        "CLICKUP_TOKEN": "your-clickup-token"
      }
    }
  }
}

